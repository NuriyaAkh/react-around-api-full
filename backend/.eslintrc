/* {
  "extends": "airbnb-base",

  "rules":{"no-underscore-dangle": ["error",{"allow":["_id"]}]}


} */
{
  "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
  },
  "extends": [
      "airbnb-base"
  ],
  "parserOptions": {
      "ecmaVersion": "latest"
  },
  "rules": {
  "no-underscore-dangle": ["off", { "allow": ["id"] }],
  "no-useless-escape": "off",
  "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
  }
}